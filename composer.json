{
  "name": "acatec/slim-skeleton",
  "description": "A Slim Framework skeleton application for rapid development",
  "keywords": [
    "microframework",
    "rest",
    "router",
    "psr7"
  ],
  "homepage": "http://github.com/acatec/Slim-Skeleton",
  "license": "MIT",
  "authors": [
    {
      "name": "Acatec",
      "email": "acatec@inbox.ru",
      "homepage": "https://github.com/acatec"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "slim/slim": "^3.1",
    "slim/php-view": "^2.0",
    "monolog/monolog": "^1.17",
    "atlas/pdo": "^1.1"
  },
  "require-dev": {
    "phpunit/phpunit": ">=4.8 < 6.0",
    "symfony/var-dumper": "^4.2"
  },
  "autoload-dev": {
    "psr-4": {
      "eduslim\\": "src/",
      "Tests\\": "tests/"
    }
  },
  "config": {
    "process-timeout": 0
  },
  "scripts": {
    "start": "php -S localhost:8080 -t public",
    "test": "phpunit"
  }
}
